program Statxyz;

uses
  System.StartUpCopy,
  FMX.Forms,
  uMain in 'uMain.pas' {fMain},
  uDistribusiBinomial in 'uDistribusiBinomial.pas' {fBinom},
  uDistribusiFrekuensi in 'uDistribusiFrekuensi.pas' {fDF},
  uRegresiLinearBerganda in 'uRegresiLinearBerganda.pas' {fRLB};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TfMain, fMain);
  Application.CreateForm(TfBinom, fBinom);
  Application.CreateForm(TfDF, fDF);
  Application.CreateForm(TfRLB, fRLB);
  Application.Run;
end.
